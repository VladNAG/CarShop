
 @{
    ViewData["Title"] = "Welcome in Catalog CARS";
}
<h1 style=" color:saddlebrown">@ViewData["Title"]</h1>
<div>
    <h>@foreach(var car in Model)
        {
            <div style="color:darkgrey">
                <img class="img-thumbnail" src="@car.img" alt="@car.Name" />
                <h2> @car.Name</h2>
                <h6>  @car.shortDesc</h6>
                <h5> Price: @car.Price $ </h5>
               
            </div>
        }
    </h>
</div>